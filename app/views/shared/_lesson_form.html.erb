<%= form_for(@lesson) do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  
  <%= f.label :date %>
  <%= f.text_field :date %>   

  <%= f.submit "Schedule", class: "btn btn-large btn-primary" %>
<% end %>

<script>
  $(function() {
    $( "#lesson_date" ).datepicker();
  });
</script>